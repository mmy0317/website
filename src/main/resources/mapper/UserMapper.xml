<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="website.project.website.mapper.UserMapper">

    <select id="selectByUserId" resultType="website.project.website.entity.UserDO">
        SELECT * FROM users WHERE user_id = #{userId}
    </select>

    <select id="selectByPhone" resultType="website.project.website.entity.UserDO">
        SELECT * FROM users WHERE phone = #{mobilePhone}
    </select>
</mapper>